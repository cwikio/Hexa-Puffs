{
  "name": "searcher-mcp-server",
  "version": "1.0.0",
  "description": "MCP server for online search using Brave Search API",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:web": "vitest run -t \"Web Search\"",
    "test:news": "vitest run -t \"News Search\""
  },
  "keywords": [
    "mcp",
    "search",
    "brave-search",
    "model-context-protocol"
  ],
  "license": "MIT",
  "dependencies": {
    "@mcp/shared": "file:../Shared",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "@mozilla/readability": "^0.6.0",
    "dotenv": "^17.2.4",
    "linkedom": "^0.18.12",
    "turndown": "^7.2.2",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "@types/turndown": "^5.0.6",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "annabelle": {
    "mcpName": "searcher",
    "sensitive": false,
    "label": "Web Search",
    "toolGroup": "Web Search",
    "keywords": ["search", "news", "weather", "forecast", "find out"],
    "guardianScan": {
      "input": false,
      "output": true
    }
  }
}
