{
  "name": "onepassword-mcp-server",
  "version": "1.0.0",
  "description": "MCP server for 1Password providing read-only vault access - list vaults, browse items, and retrieve secrets securely",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:unit": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "mcp",
    "1password",
    "secrets",
    "model-context-protocol"
  ],
  "license": "MIT",
  "dependencies": {
    "@mcp/shared": "file:../Shared",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "annabelle": {
    "mcpName": "onepassword",
    "transport": "stdio",
    "sensitive": true,
    "label": "1Password",
    "toolGroup": "Secrets",
    "keywords": ["password", "secret", "credential", "vault", "1password"],
    "guardianScan": {
      "input": true,
      "output": false
    }
  }
}
